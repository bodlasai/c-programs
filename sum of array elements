//sum of array elements
#include<stdio.h>
int sum(int arr[],int n);
int main()
{
    int arr[]={1,2,3,4,5};
    int n=sizeof(arr)/sizeof(arr[0]);
    printf("the sum of array values %d",sum(arr,n));
    return 0;
    
}
int sum(int arr[],int n)
{
    int sum=0,i;
    for(i=1;i<=n;i++)
    {
        sum=sum+arr[i];
    }
    return sum;
}
